{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "clean",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "${workspaceFolder}/CSMOO.sln"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "--configuration",
        "Debug",
        "${workspaceFolder}/CSMOO.sln"
      ],
      "problemMatcher": "$msCompile",
      "dependsOn": ["clean"]
    }
  ]
}
